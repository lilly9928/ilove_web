function adduser(){
    let today = new Date();  
    var new_manager = new Object();
    new_manager.user_id =$('#inputUserId').val()
    new_manager.user_password=$('#inputPassword').val()
    new_manager.user_nickname=$('#inputNickname').val()
    new_manager.user_birthday=$('#inputbirthday').val()
    new_manager.user_gender=$('#inputgender').val()
    new_manager.user_purpose=$('#inputpurpose').val()
    new_manager.user_authority='manager'

   
    new_manager.user_introduce=$('#user_introduce').val()

    new_manager.user_phone=$('#inputUserId').val()
    new_manager.user_city=$('#regidence').val()
    new_manager.user_recenttime=today

    new_manager.user_recentgps=$('#inputUserId').val()


    new_manager.user_signdate=today

    new_manager.user_previewintroduce=$('#previewintroduce').val()

    var new_manager_option = new Object();
    new_manager_option.user_id=$('#inputUserId').val()
    new_manager_option.user_height=$('#height').val()
    new_manager_option.user_bodytype=$('#bodyType').val()
    new_manager_option.user_bloodtype=$('#bloodType').val()
    new_manager_option.user_residence=$('#residence').val()
    new_manager_option.user_job=$('#job').val()
    new_manager_option.user_education=$('#education').val()
    new_manager_option.user_holiday=$('#holiday').val()
    new_manager_option.user_cigarette=$('#cigarette').val()
    new_manager_option.user_alcohol=$('#alcohol').val()
    new_manager_option.user_religion=$('#religion').val()
    new_manager_option.user_brother=$('#brother').val()
    new_manager_option.user_country=$('#country').val()
    new_manager_option.user_salary=$('#salary').val()
    new_manager_option.user_asset=$('#asset').val()
    new_manager_option.user_marriagehistory=$('#marriagehistory').val()
    new_manager_option.user_children=$('#children').val()
    new_manager_option.user_marriageplan=$('#marriageplan').val()
    new_manager_option.user_childrenplan=$('#childrenplan').val()
   
    new_manager_option.user_wishdate=$('#wishdate').val()
    new_manager_option.user_datecost=$('#costdate').val()
    new_manager_option.user_roommate=$('#roommate').val()
   
    new_manager_option.user_interest=$('#interest_text').val()
    new_manager_option.user_personality=$('#personality').val()
    new_manager_option.user_favoriteperson=$('#favoriteperson_text').val()

    a2s_addDB('user', JSON.stringify(new_manager), function (res) {
