}

$("#interest").change(function(){
    var data=$("#interest_text").val()
    if(data!=''){
    $("#interest_text").val(data+','+$(this).val());
    }
    else  $("#interest_text").val($(this).val()); 
})

$("#personality").change(function(){
    var data=$("#personality_text").val()
