                a2s_addDB('useroption', JSON.stringify(new_manager_option), function (res) {
                if (res.affectedRows > 0) {
                    alert('회원가입이 완료되었습니다.')
                    location.href="/"
                }
                else {
                    alert('회원가입에 실패하였습니다.')
                }
        })
            }
            else {
                alert('회원가입에 실패하였습니다.')
            }
           
        })

}
